{
  "extends": ["airbnb", "plugin:flowtype/recommended", "prettier", "prettier/react"],
  "plugins": ["flowtype", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"],
        "no-restricted-syntax": [
          "error",
          "ForInStatement",
          "LabeledStatement",
          "WithStatement"
        ]
      }
    ],
    "linebreak-style": 0,
    "prettier/prettier": "error",
    "no-dupe-keys": 0,
    "no-console": 0,
    "react/prefer-stateless-function": 0,
    "node/no-unsupported-features/es-syntax": 0

  }
}